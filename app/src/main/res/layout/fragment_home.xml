                        android:layout_width="200dp"
                        android:layout_height="100dp"
                        android:layout_marginStart="1dp"
                        android:layout_marginEnd="5dp"
                        android:src="@drawable/test" />

                    <ImageView
                        android:id="@+id/small_ads_4"
                        android:layout_width="200dp"
                        android:layout_height="100dp"
                        android:src="@drawable/test" />

                </LinearLayout>

            </LinearLayout>

